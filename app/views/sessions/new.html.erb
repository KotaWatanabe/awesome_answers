<div class="container-fluid padding">
<h1>Sign in</h1>

<%= form_with url: session_path, local: true do |f| %>
  <div>
    <%= f.label :email %> <br>
    <%= f.email_field :email %>
  </div>
  <div>
    <%= f.label :password %> <br>
    <%= f.password_field :password %>
  </div>
  <%= f.submit "Sign In" %>
<% end %>
</div>
